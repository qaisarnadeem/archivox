
<%= render :partial => 'form' %>
<%- model_class = EventSession -%>
<div class="col-md-9 col-md-offset-2">
<div class="page-header " style="width:83%">
  <h2>Active Sessions</h2>
</div>
<table class="table table-striped" style="width:83%">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:code) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @event_sessions.each do |event_session| %>
      <tr>
        <td><%= event_session.title %></td>
        <td><%= event_session.code %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_event_session_path(event_session), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.delete")),
                      event_session_path(event_session),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
